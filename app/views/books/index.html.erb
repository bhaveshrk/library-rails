<%= render 'layouts/dash'%>
<p style="color: green"><%= notice %></p>
<h1>Books</h1>
<div>
<table class="table table-striped table-bordered table-hover">

  <thead>
    <tr>
      <th>Title:</th>
      <th>Published year:
      </th>
      <th>Author:
      </th>
    </tr>
  </thead>

<div id="books"> 
    <% @books.each do |book| %>
    <%= render book %>
    <% end %>
</div>
</table>

<%= link_to "New book", new_book_path %>
</div>



